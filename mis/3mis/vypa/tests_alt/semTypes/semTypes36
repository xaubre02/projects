/* Invalid type in condition - string */
class Shape : Object {
	int id;
    string stringAttr;

	void Shape(void) {
		print("constructor of Shape");
	}

	string toString(void) {
		return "instance of Shape " + (string)(this.id);
	}

    void hello(string s) {
        print(s);
    }
}

void main(void) {
    Shape s;
    s = new Shape;

    int i;
    while (s.toString()) {
        i = 5;
    }

    return;
}