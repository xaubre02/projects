/* Invalid type of operation NOT(!) - void */

class Shape : Object {
	int id;
    string stringAttr;

	void Shape(void) {
		print("constructor of Shape");
	}

	string toString(void) {
		return "instance of Shape " + (string)(this.id);
	}

    void hello(string s) {
        print(s);
    }
}

void main(void) {
    Shape s;
    s = new Shape;
    int i;
    while(!(s.hello("NoWay"))) {
        i = 42;
    }
    
    return;
}
